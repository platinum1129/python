{
    "AWSTemplateFormatVersion": "2010-09-09",
    "Description": "20201022",
    "Parameters": {
        "LatestS3Bucket": {
            "Type": "String"
        },
        "LatestS3Key": {
            "Type": "String"
        }
    },
    "Resources": {
        "PostZoomMeeting1022": {
            "Type": "AWS::Lambda::Function",
            "Properties": {
                "FunctionName": "PostZoomMeeting1022",
                "Code": {
                    "S3Bucket": {
                        "Ref": "LatestS3Bucket"
                    },
                    "S3Key": {
                        "Ref": "LatestS3Key"
                    }
                },
                "Description": "create by codepipeline",
                "Handler": "PostZoomMeeting.lambda_handler",
                "Role": "arn:aws:iam::674607486925:role/MatsuLambdaRole",
                "Runtime": "python3.8",
                "Environment": {
                    "Variables": {
                        "SLACK_OAUTH_TOKEN": "{{resolve:ssm-secure:SLACK_OAUTH_TOKEN:1}}",
                        "SLACK_BOT_TOKEN": "{{resolve:ssm-secure:SLACK_BOT_TOKEN:1}}",
                        "SLACK_USER_NAME": "{{resolve:ssm-secure:SLACK_USER_NAME:1}}",
                        "SLACK_CHANNEL_TEST": "{{resolve:ssm-secure:SLACK_CHANNEL_TEST:1}}",
                        "SLACK_CHANNEL_MAIN": "{{resolve:ssm-secure:SLACK_CHANNEL_MAIN:1}}",
                        "ZOOM_API_KEY": "{{resolve:ssm-secure:ZOOM_API_KEY:1}}",
                        "ZOOM_API_SECRET": "{{resolve:ssm-secure:ZOOM_API_SECRET:1}}",
                        "ZOOM_USER_ID": "{{resolve:ssm-secure:ZOOM_USER_ID:1}}"
                    }
                },
                "Timeout": 10,
                "MemorySize": 128
            }
        }
    }
}
